{
    "name": "mimetic",
    "version": "0.5.7",
    "description": "Scalable Fonts & Zoom Detection",
    "main": "dist/mimetic.js",
    "jsnext:main": "dist/mimetic.es.js",
    "scripts": {
        "watch": "concurrently \"rollup -c ./rollup/rollup.config.umd.js -w\" \"rollup -c ./rollup/rollup.config.es.js -w\"",
        "build": "rollup -c ./rollup/rollup.config.umd.js && rollup -c ./rollup/rollup.config.es.js",
        "build-client-test": "watchify ./test/*.js -o ./test/_client-test.js -t [ babelify --presets [ es2015 ] ] -v",
        "test": "node gaze.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/julienetie/mimetic.git"
    },
    "author": "Julien Etienne",
    "license": "MIT",
    "devDependencies": {
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015-rollup": "^1.2.0",
        "babelify": "^7.3.0",
        "browserify": "^14.3.0",
        "concurrently": "^3.4.0",
        "express": "^4.15.2",
        "gaze": "^1.1.2",
        "gulp": "^3.9.1",
        "gulp-bump": "^2.2.0",
        "gulp-concat": "^2.6.0",
        "gulp-header": "^1.8.7",
        "gulp-notify": "^2.2.0",
        "gulp-plumber": "^1.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-uglify": "^1.5.4",
        "rollup-plugin-babel": "^2.6.1",
        "rollup-plugin-multi-entry": "^2.0.1",
        "rollup-plugin-node-resolve": "^2.0.0",
        "rollup-watch": "^2.5.0",
        "watchify": "^3.9.0"
    },
    "dependencies": {
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.24.1",
        "chai": "^3.5.0",
        "jsdom": "*",
        "mocha": "^3.2.0",
        "resizilla": "^0.9.2",
        "round-to": "^2.0.0",
        "run-once": "*"
    }
}
