{
    "name": "mimetic",
    "version": "0.5.0",
    "description": "Scalable Fonts & Zoom Detection",
    "main": "dist/mimetic.js",
    "scripts": {
        "watch": "concurrently \"rollup -c ./rollup/rollup.config.umd.js -w\" \"rollup -c ./rollup/rollup.config.es.js -w\"",
        "build": "rollup -c ./rollup/rollup.config.umd.js && rollup -c ./rollup/rollup.config.es.js",
        "test": "mocha --compilers js:babel-register --recursive"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/julienetie/mimetic.git"
    },
    "author": "Julien Etienne",
    "license": "MIT",
    "devDependencies": {
        "babel-preset-es2015-rollup": "^1.2.0",
        "concurrently": "^3.4.0",
        "gulp": "^3.9.1",
        "gulp-bump": "^2.2.0",
        "gulp-concat": "^2.6.0",
        "gulp-header": "^1.8.7",
        "gulp-notify": "^2.2.0",
        "gulp-plumber": "^1.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-uglify": "^1.5.4",
        "rollup-plugin-babel": "^2.6.1",
        "rollup-plugin-node-resolve": "^2.0.0",
        "rollup-watch": "^2.5.0"
    },
    "dependencies": {
        "babel-preset-es2015": "*",
        "babel-register": "*",
        "chai": "^3.5.0",
        "jsdom": "*",
        "mocha": "*",
        "resizilla": "^0.9.2",
        "round-to": "^2.0.0",
        "run-once": "*"
    }
}
